/*
    Test profile for NBIS AnnotationPreprocessing pipeline

    Usage:
        nextflow run -profile test NBISweden/pipelines-nextflow/pipelines/AnnotationPreprocessing
*/
params {
    genome = 'https://github.com/nf-core/test-datasets/raw/rnaseq/reference/genome.fa'
    outdir = "results"
    min_length = 1000
    busco_lineage = [ 'eukaryota_odb10', 'bacteria_odb10' ]
}
